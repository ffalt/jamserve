{
  "name": "jamserve",
  "version": "0.2.24-alpha",
  "author": "ffalt",
  "license": "MIT",
  "description": "jamserve streaming audio server",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/ffalt/jamserve.git"
  },
  "bin": {
    "jamserve": "./dist/bundle.js"
  },
  "scripts": {
    "start": "node dist/bundle.js",
    "develop": "concurrently --kill-others \"npm run build:dev:watch\" \"npm run develop:watch\"",
    "develop:watch": "nodemon --config nodemon.json",
    "cleandb": "node dist/bundle.js --cleardb",
    "lint": "eslint ./src --ext .js,.ts",
    "lint:debug": "DEBUG=eslint:cli-engine eslint ./src --ext .js,.ts",
    "clean": "rimraf dist",
    "test": "jest --maxWorkers=1",
    "test:watch": "jest --maxWorkers=1 --watch",
    "coverage": "jest --maxWorkers=1 --coverage",
    "release": "deplokay -c ./deplokay.json",
    "build:changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "build:prod": "webpack --mode production",
    "build:deploy": "npm run clean && npm run build:prod && npm run build:docs",
    "build:dev": "webpack --mode development",
    "build:dev:watch": "webpack --watch --mode development",
    "build:docs": "npm run build:jam:api:docs",
    "build:model": "npm run build:model:jam",
    "build:model:jam": "npm run build:jam:openapi && npm run build:jam:elastic && npm run build:jam:client:angular && npm run build:jam:client:axios && npm run build:jam:router && npm run build:jam:tests",
    "build:jam:tests": "cd dev/bin && ts-node update-jam-openapi-test.ts",
    "build:jam:openapi": "cd dev/bin && ts-node update-jam-openapi.ts",
    "build:jam:elastic": "cd dev/bin && ts-node update-elastic-mapping.ts",
    "build:jam:client:angular": "cd dev/bin && ts-node update-jam-client.ts",
    "build:jam:client:axios": "cd dev/bin && ts-node update-jam-client-axios.ts",
    "build:jam:router": "cd dev/bin && ts-node update-jam-router.ts",
    "build:jam:api:docs": "redoc-cli bundle src/model/jam-openapi.json --output ./dist/docs/api/jam/index.html"
  },
  "dependencies": {
    "@elastic/elasticsearch": "7.6.0",
    "ajv": "6.12.0",
    "archiver": "3.1.1",
    "body-parser": "1.19.0",
    "colors": "1.4.0",
    "commander": "4.1.1",
    "common-password-checker": "0.1.0",
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "d3": "5.15.0",
    "d3-array": "2.4.0",
    "d3-node": "2.2.1",
    "d3-scale": "3.2.1",
    "d3-shape": "1.3.7",
    "eslint": "6.8.0",
    "express": "4.17.1",
    "express-rate-limit": "5.1.1",
    "express-session": "1.17.0",
    "express-useragent": "1.0.13",
    "feedparser": "2.2.9",
    "fluent-ffmpeg": "2.1.2",
    "fs-extra": "8.1.0",
    "helmet": "3.21.3",
    "iconv-lite": "0.5.1",
    "jamp3": "0.4.1",
    "jimp": "0.9.3",
    "json-schema-ref-parser": "7.1.3",
    "jsonwebtoken": "8.5.1",
    "limiter": "1.1.5",
    "mime-types": "2.1.26",
    "moment": "2.24.0",
    "multer": "1.4.2",
    "nedb": "1.8.0",
    "node-worker-threads-pool": "1.1.2",
    "on-finished": "2.3.0",
    "p-queue": "6.3.0",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "request": "2.88.2",
    "seedrandom": "3.0.5",
    "sharp": "0.24.1",
    "svgo": "1.3.2",
    "tmp": "0.1.0",
    "waveform-data": "3.3.1",
    "which": "2.0.2",
    "winston": "3.2.1",
    "xml2js": "0.4.23",
    "yauzl": "2.10.0"
  },
  "devDependencies": {
    "@types/archiver": "3.1.0",
    "@types/chance": "1.0.8",
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.6",
    "@types/d3": "5.7.2",
    "@types/express": "4.17.2",
    "@types/express-session": "1.17.0",
    "@types/express-useragent": "1.0.0",
    "@types/feedparser": "2.2.3",
    "@types/fluent-ffmpeg": "2.1.14",
    "@types/fs-extra": "8.1.0",
    "@types/helmet": "0.0.45",
    "@types/jest": "25.1.3",
    "@types/jest-expect-message": "1.0.1",
    "@types/mime-types": "2.1.0",
    "@types/multer": "1.4.2",
    "@types/mustache": "4.0.1",
    "@types/nedb": "1.8.9",
    "@types/nock": "11.1.0",
    "@types/node": "13.9.1",
    "@types/on-finished": "2.3.1",
    "@types/passport": "1.0.2",
    "@types/passport-jwt": "3.0.3",
    "@types/passport-local": "1.0.33",
    "@types/request": "2.48.4",
    "@types/rimraf": "2.0.3",
    "@types/seedrandom": "2.4.28",
    "@types/sharp": "0.24.0",
    "@types/supertest": "2.0.8",
    "@types/svgo": "1.3.1",
    "@types/tmp": "0.1.0",
    "@types/which": "1.3.2",
    "@types/xml2js": "0.4.5",
    "@types/yauzl": "2.9.1",
    "@typescript-eslint/eslint-plugin": "2.21.0",
    "@typescript-eslint/parser": "2.21.0",
    "chance": "1.1.4",
    "concurrently": "5.1.0",
    "conventional-changelog-cli": "2.0.31",
    "copy-webpack-plugin": "5.1.1",
    "deplokay": "0.0.12",
    "jest": "25.1.0",
    "jest-expect-message": "1.0.2",
    "mustache": "4.0.0",
    "nock": "12.0.1",
    "nodemon": "2.0.2",
    "redoc-cli": "0.9.6",
    "rimraf": "3.0.2",
    "source-map-support": "0.5.16",
    "supertest": "4.0.2",
    "ts-jest": "25.2.1",
    "ts-loader": "6.2.1",
    "ts-node": "8.6.2",
    "typescript": "3.8.2",
    "typescript-json-schema": "0.42.0",
    "webpack": "4.41.6",
    "webpack-cli": "3.3.11",
    "webpack-filter-warnings-plugin": "1.2.1",
    "webpack-node-externals": "1.7.2"
  },
  "engines": {
    "node": ">= 11.0.0"
  }
}
