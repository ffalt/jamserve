{
  "name": "jamserve",
  "version": "0.1.8-alpha",
  "author": "ffalt",
  "license": "MIT",
  "description": "jamserve streaming audio server",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/ffalt/jamserve.git"
  },
  "bin": {
    "jamserve": "./dist/bundle.js"
  },
  "scripts": {
    "start": "node dist/bundle.js",
    "build": "webpack --mode development",
    "build:prod": "webpack --mode production",
    "build:docs": "npm run dev:docs:api:jam && npm run dev:docs:api:subsonic",
    "develop": "concurrently --kill-others \"npm run dev:watch\" \"npm run dev:nodemon\"",
    "build:deploy": "npm run clean && npm run build:prod && npm run build:docs",
    "cmd:cleandb": "node dist/bundle.js --cleardb",
    "dev:nodemon": "nodemon --config nodemon.json",
    "dev:watch": "webpack --watch --mode development",
    "dev:lint": "tslint --project tsconfig.json \"src/**/*.ts\"",
    "dev:lint:fix": "tslint --fix --project tsconfig.json \"src/**/*.ts\"",
    "dev:update:schema:jam": "cd dev/bin && ts-node update-jam-schema.ts",
    "dev:update:schema:subsonic": "cd dev/bin && ts-node update-subsonic-schema.ts",
    "dev:update:elasticmapping": "cd dev/bin && ts-node update-elastic-mapping.ts",
    "dev:update:openapi:jam": "cd dev/bin && ts-node update-jam-openapi.ts",
    "dev:update:openapi:subsonic": "cd dev/bin && ts-node update-subsonic-openapi.ts",
    "dev:update:client:jam": "cd dev/bin && ts-node update-jam-client.ts",
    "dev:update:router:jam": "cd dev/bin && ts-node update-jam-router.ts",
    "dev:update:router:subsonic": "cd dev/bin && ts-node update-subsonic-router.ts",
    "dev:update:jam": "npm run dev:update:schema:jam && npm run dev:update:openapi:jam && npm run dev:update:client:jam && npm run dev:update:router:jam",
    "dev:update:subsonic": "npm run dev:update:schema:subsonic && npm run dev:update:openapi:subsonic && npm run dev:update:router:subsonic",
    "dev:update": "npm run dev:update:jam && npm run dev:update:subsonic && npm run dev:update:elasticmapping",
    "dev:docs:api:subsonic": "redoc-cli bundle src/model/subsonic-openapi.json --output ./dist/docs/api/subsonic/index.html",
    "dev:docs:api:jam": "redoc-cli bundle src/model/jam-openapi.json --output ./dist/docs/api/jam/index.html",
    "clean": "rimraf dist",
    "test:services": "mocha --recursive \".src/**/**.service.test.ts\"",
    "test:stores": "mocha --recursive \".src/**/**.store.test.ts\"",
    "test": "mocha --recursive \"./src/**/**.test.ts\"",
    "coverage": "nyc mocha --recursive \"./src/**/**.test.ts\"",
    "release": "deplokay -c ./deplokay.json"
  },
  "dependencies": {
    "ajv": "6.10.0",
    "archiver": "3.0.0",
    "body-parser": "1.19.0",
    "colors": "1.3.3",
    "commander": "2.20.0",
    "common-password-checker": "0.1.0",
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "elasticsearch": "16.1.1",
    "express": "4.17.1",
    "express-rate-limit": "5.0.0",
    "express-session": "1.16.2",
    "feedparser": "2.2.9",
    "fluent-ffmpeg": "2.1.2",
    "fs-extra": "8.0.1",
    "helmet": "3.18.0",
    "iconv-lite": "0.4.24",
    "jamp3": "0.3.10",
    "jimp": "0.6.4",
    "jsonwebtoken": "8.5.1",
    "limiter": "1.1.4",
    "md5-typescript": "1.0.5",
    "mime-types": "2.1.24",
    "moment": "2.24.0",
    "multer": "1.4.1",
    "multer-autoreap": "1.0.3",
    "nedb": "1.8.0",
    "passport": "0.4.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "request": "2.88.0",
    "seedrandom": "3.0.1",
    "sharp": "0.22.1",
    "svgo": "1.2.2",
    "tmp": "0.1.0",
    "waveform-data": "2.1.2",
    "winston": "3.2.1",
    "xml2js": "0.4.19"
  },
  "devDependencies": {
    "@types/archiver": "3.0.0",
    "@types/chai": "4.1.7",
    "@types/chai-as-promised": "7.1.0",
    "@types/cookie-parser": "1.4.1",
    "@types/node-memwatch": "^1.0.1",
    "@types/cors": "2.8.5",
    "@types/elasticsearch": "5.0.34",
    "@types/express": "4.17.0",
    "@types/express-session": "1.15.12",
    "@types/feedparser": "2.2.2",
    "@types/fluent-ffmpeg": "2.1.9",
    "@types/fs-extra": "7.0.0",
    "@types/helmet": "0.0.43",
    "@types/image-size": "^0.7.0",
    "@types/json-stable-stringify": "1.0.32",
    "@types/mime-types": "2.1.0",
    "@types/mkdirp": "0.5.2",
    "@types/mocha": "5.2.7",
    "@types/multer": "1.3.7",
    "@types/nedb": "1.8.7",
    "@types/nock": "10.0.3",
    "@types/node": "12.0.8",
    "@types/passport": "1.0.0",
    "@types/passport-jwt": "3.0.1",
    "@types/passport-local": "1.0.33",
    "@types/request": "2.48.1",
    "@types/rimraf": "2.0.2",
    "@types/seedrandom": "2.4.28",
    "@types/sharp": "0.22.2",
    "@types/supertest": "2.0.7",
    "@types/svgo": "1.0.2",
    "@types/tmp": "0.1.0",
    "@types/xml2js": "0.4.4",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "concurrently": "4.1.0",
    "copy-webpack-plugin": "5.0.3",
    "node-memwatch": "1.0.1",
    "deplokay": "0.0.6",
    "mocha": "6.1.4",
    "nock": "10.0.6",
    "nodemon": "1.19.1",
    "nyc": "14.1.1",
    "redoc-cli": "0.8.4",
    "rimraf": "2.6.3",
    "source-map-support": "0.5.12",
    "supertest": "4.0.2",
    "ts-loader": "6.0.2",
    "ts-node": "8.2.0",
    "tslint": "5.17.0",
    "typescript": "3.5.1",
    "typescript-json-schema": "0.38.3",
    "webpack": "4.33.0",
    "webpack-cli": "3.3.4",
    "webpack-filter-warnings-plugin": "1.2.1",
    "webpack-node-externals": "1.7.2"
  },
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "files": [
      "test/**/*.ts"
    ]
  },
  "nyc": {
    "temp-dir": "./reports/.nyc_output",
    "report-dir": "./reports/coverage",
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.mock.ts",
      "**/*.test.ts",
      "**/*.spec.ts"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "engines": {
    "node": ">= 0.8.10"
  }
}
