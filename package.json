{
  "name": "jamserve",
  "version": "0.2.0-alpha",
  "author": "ffalt",
  "license": "MIT",
  "description": "jamserve streaming audio server",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/ffalt/jamserve.git"
  },
  "bin": {
    "jamserve": "./dist/bundle.js"
  },
  "scripts": {
    "start": "node dist/bundle.js",
    "develop": "concurrently --kill-others \"npm run build:dev:watch\" \"npm run develop:watch\"",
    "develop:watch": "nodemon --config nodemon.json",
    "cleandb": "node dist/bundle.js --cleardb",
    "lint": "tslint --project tsconfig.json \"src/**/*.ts\"",
    "lint:fix": "tslint --fix --project tsconfig.json \"src/**/*.ts\"",
    "clean": "rimraf dist",
    "test": "jest --maxWorkers=2",
    "test:watch": "jest --maxWorkers=2 --watch",
    "coverage": "jest --maxWorkers=2 --coverage",
    "release": "deplokay -c ./deplokay.json",
    "build:prod": "webpack --mode production",
    "build:deploy": "npm run clean && npm run build:prod && npm run build:docs",
    "build:dev": "webpack --mode development",
    "build:dev:watch": "webpack --watch --mode development",
    "build:docs": "npm run build:jam:api:docs && npm run build:subsonic:api:docs",
    "build:model": "npm run build:model:jam && npm run build:model:subsonic",
    "build:model:jam": "npm run build:jam:openapi && npm run build:jam:elastic && npm run build:jam:client && npm run build:jam:router",
    "build:model:subsonic": "npm run build:subsonic:openapi && npm run build:subsonic:router",
    "build:jam:tests": "cd dev/bin && ts-node update-jam-openapi-test.ts",
    "build:jam:openapi": "cd dev/bin && ts-node update-jam-openapi.ts",
    "build:jam:elastic": "cd dev/bin && ts-node update-elastic-mapping.ts",
    "build:jam:client": "cd dev/bin && ts-node update-jam-client.ts",
    "build:jam:router": "cd dev/bin && ts-node update-jam-router.ts",
    "build:jam:api:docs": "redoc-cli bundle src/model/jam-openapi.json --output ./dist/docs/api/jam/index.html",
    "build:subsonic:openapi": "cd dev/bin && ts-node update-subsonic-openapi.ts",
    "build:subsonic:router": "cd dev/bin && ts-node update-subsonic-router.ts",
    "build:subsonic:api:docs": "redoc-cli bundle src/model/subsonic-openapi.json --output ./dist/docs/api/subsonic/index.html"
  },
  "dependencies": {
    "ajv": "6.10.2",
    "archiver": "3.1.1",
    "body-parser": "1.19.0",
    "colors": "1.3.3",
    "commander": "3.0.1",
    "common-password-checker": "0.1.0",
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "d3": "5.12.0",
    "d3-array": "2.3.1",
    "d3-node": "2.2.0",
    "d3-scale": "3.1.0",
    "d3-shape": "1.3.5",
    "elasticsearch": "16.3.0",
    "express": "4.17.1",
    "express-rate-limit": "5.0.0",
    "express-session": "1.16.2",
    "express-useragent": "1.0.13",
    "feedparser": "2.2.9",
    "fluent-ffmpeg": "2.1.2",
    "fs-extra": "8.1.0",
    "helmet": "3.21.0",
    "iconv-lite": "0.5.0",
    "jamp3": "0.4.0",
    "jimp": "0.8.2",
    "json-schema-ref-parser": "7.1.1",
    "jsonwebtoken": "8.5.1",
    "limiter": "1.1.4",
    "md5-typescript": "1.0.5",
    "mime-types": "2.1.24",
    "moment": "2.24.0",
    "multer": "1.4.2",
    "nedb": "1.8.0",
    "on-finished": "2.3.0",
    "passport": "0.4.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "request": "2.88.0",
    "seedrandom": "3.0.3",
    "sharp": "0.23.0",
    "svgo": "1.3.0",
    "tmp": "0.1.0",
    "waveform-data": "3.0.0",
    "which": "1.3.1",
    "winston": "3.2.1",
    "xml2js": "0.4.22"
  },
  "devDependencies": {
    "@types/archiver": "3.0.0",
    "@types/chance": "1.0.6",
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.6",
    "@types/d3": "5.7.2",
    "@types/elasticsearch": "5.0.35",
    "@types/express": "4.17.1",
    "@types/express-session": "1.15.14",
    "@types/express-useragent": "0.2.22",
    "@types/feedparser": "2.2.3",
    "@types/fluent-ffmpeg": "2.1.10",
    "@types/fs-extra": "8.0.0",
    "@types/helmet": "0.0.44",
    "@types/jest": "24.0.18",
    "@types/mime-types": "2.1.0",
    "@types/multer": "1.3.9",
    "@types/mustache": "0.8.32",
    "@types/nedb": "1.8.9",
    "@types/nock": "11.1.0",
    "@types/node": "12.7.5",
    "@types/on-finished": "2.3.1",
    "@types/passport": "1.0.1",
    "@types/passport-jwt": "3.0.2",
    "@types/passport-local": "1.0.33",
    "@types/request": "2.48.3",
    "@types/rimraf": "2.0.2",
    "@types/seedrandom": "2.4.28",
    "@types/sharp": "0.22.2",
    "@types/supertest": "2.0.8",
    "@types/svgo": "1.2.1",
    "@types/tmp": "0.1.0",
    "@types/xml2js": "0.4.5",
    "@types/which": "1.3.1",
    "chance": "1.1.0",
    "concurrently": "4.1.2",
    "copy-webpack-plugin": "5.0.4",
    "deplokay": "0.0.9",
    "jest": "24.9.0",
    "mustache": "3.1.0",
    "nock": "11.3.4",
    "nodemon": "1.19.2",
    "redoc-cli": "0.8.6",
    "rimraf": "3.0.0",
    "source-map-support": "0.5.13",
    "supertest": "4.0.2",
    "ts-jest": "24.1.0",
    "ts-loader": "6.1.0",
    "ts-node": "8.3.0",
    "tslint": "5.20.0",
    "typescript": "3.6.3",
    "typescript-json-schema": "0.40.0",
    "webpack": "4.40.2",
    "webpack-cli": "3.3.8",
    "webpack-filter-warnings-plugin": "1.2.1",
    "webpack-node-externals": "1.7.2"
  },
  "engines": {
    "node": ">= 0.8.10"
  }
}
