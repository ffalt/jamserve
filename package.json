{
  "name": "jamserve",
  "version": "0.3.0-alpha",
  "author": "ffalt",
  "license": "MIT",
  "description": "jamserve streaming audio server",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/ffalt/jamserve.git"
  },
  "scripts": {
    "start": "node dist/index.js",
    "develop": "tsc-watch --onSuccess \"node ./dist/index.js\"",
    "develop:tsnode": "ts-node-dev src/index.ts",
    "lint": "eslint ./src --ext .js,.ts",
    "lint:fix": "eslint ./src --ext .js,.ts --fix",
    "build": "tsc",
    "build:prod": "npm run clean && npm run build",
    "build:deploy": "npm run build:prod && ts-node deploy.ts",
    "clean": "rimraf dist",
    "test": "jest --maxWorkers=1",
    "test:watch": "jest --maxWorkers=1 --watch",
    "coverage": "jest --runInBand --coverage",
    "orm:cache:create": "mikro-orm cache:generate",
    "orm:cache:clear": "mikro-orm cache:clear",
    "orm:dump": "mikro-orm generate-entities",
    "orm:schema:create": "mikro-orm schema:create",
    "orm:schema:drop": "mikro-orm schema:drop",
    "orm:schema:update": "mikro-orm schema:update --run"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/config/orm.config.ts",
      "./dist/config/orm.config.js"
    ]
  },
  "dependencies": {
    "apollo-server-express": "2.16.0",
    "apollo-server-logger": "1.0.1",
    "archiver": "4.0.2",
    "body-parser": "1.19.0",
    "class-validator": "0.12.2",
    "common-password-checker": "0.1.0",
    "compression": "1.7.4",
    "console-table-printer": "2.0.2",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "d3": "5.16.0",
    "d3-array": "2.4.0",
    "d3-node": "2.2.2",
    "d3-scale": "3.2.1",
    "d3-shape": "1.3.7",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-rate-limit": "5.1.3",
    "express-session": "1.17.1",
    "express-useragent": "1.0.15",
    "feedparser": "2.2.10",
    "fluent-ffmpeg": "2.1.2",
    "fs-extra": "9.0.1",
    "graphql": "15.3.0",
    "graphql-type-json": "0.3.2",
    "helmet": "3.23.3",
    "jamp3": "0.4.1",
    "jimp": "0.14.0",
    "limiter": "1.1.5",
    "method-override": "3.0.0",
    "mikro-orm": "3.6.15",
    "mime-types": "2.1.27",
    "moment": "2.27.0",
    "multer": "1.4.2",
    "mustache": "4.0.1",
    "node-worker-threads-pool": "1.3.3",
    "on-finished": "2.3.0",
    "p-queue": "6.5.0",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "reflect-metadata": "0.1.13",
    "request": "2.88.2",
    "seedrandom": "3.0.5",
    "sharp": "0.25.4",
    "sqlite3": "5.0.0",
    "type-graphql": "1.0.0-rc.3",
    "typescript-ioc": "3.2.2",
    "waveform-data": "3.3.2",
    "which": "2.0.2",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@apidevtools/json-schema-ref-parser": "9.0.3",
    "@types/archiver": "3.1.0",
    "@types/chance": "1.1.0",
    "@types/compression": "1.7.0",
    "@types/cookie-parser": "1.4.2",
    "@types/d3": "5.7.2",
    "@types/express": "4.17.7",
    "@types/express-session": "1.17.0",
    "@types/express-useragent": "1.0.0",
    "@types/feedparser": "2.2.3",
    "@types/fluent-ffmpeg": "2.1.14",
    "@types/fs-extra": "9.0.1",
    "@types/graphql-type-json": "0.3.2",
    "@types/helmet": "0.0.47",
    "@types/jest": "26.0.4",
    "@types/jest-expect-message": "1.0.2",
    "@types/method-override": "0.0.31",
    "@types/mime-types": "2.1.0",
    "@types/multer": "1.4.3",
    "@types/mustache": "4.0.1",
    "@types/node": "14.0.26",
    "@types/on-finished": "2.3.1",
    "@types/passport": "1.0.4",
    "@types/passport-jwt": "3.0.3",
    "@types/passport-local": "1.0.33",
    "@types/request": "2.48.5",
    "@types/seedrandom": "2.4.28",
    "@types/sharp": "0.25.0",
    "@types/supertest": "2.0.10",
    "@types/tmp": "0.2.0",
    "@types/uuid": "8.0.0",
    "@types/which": "1.3.2",
    "@typescript-eslint/eslint-plugin": "3.6.1",
    "@typescript-eslint/parser": "3.6.1",
    "chance": "1.1.6",
    "eslint": "7.4.0",
    "jest": "26.1.0",
    "jest-expect-message": "1.0.2",
    "jest-openapi": "0.9.4",
    "nock": "13.0.2",
    "openapi3-ts": "1.4.0",
    "supertest": "4.0.2",
    "tmp": "0.2.1",
    "ts-jest": "26.1.3",
    "ts-node": "8.10.2",
    "ts-node-dev": "1.0.0-pre.52",
    "tsc-watch": "4.2.9",
    "typescript": "3.9.7"
  }
}
