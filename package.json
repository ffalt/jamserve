    "d3-array": "3.1.1",
    "d3-scale": "4.0.2",
    "d3-selection": "3.0.0",
    "d3-shape": "3.0.1",
    "dotenv": "10.0.0",
    "express": "4.17.1",
    "express-session": "1.17.2",
    "express-useragent": "1.0.15",
    "feedparser": "2.2.10",
