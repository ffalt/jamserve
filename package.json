{
  "name": "jamserve",
  "version": "0.2.0-alpha",
  "author": "ffalt",
  "license": "MIT",
  "description": "jamserve streaming audio server",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/ffalt/jamserve.git"
  },
  "bin": {
    "jamserve": "./dist/bundle.js"
  },
  "scripts": {
    "start": "node dist/bundle.js",
    "build": "webpack --mode development",
    "build:prod": "webpack --mode production",
    "build:docs": "npm run docs:api:jam && npm run docs:api:subsonic",
    "develop": "concurrently --kill-others \"npm run watch\" \"npm run nodemon\"",
    "build:deploy": "npm run clean && npm run build:prod && npm run build:docs",
    "cmd:cleandb": "node dist/bundle.js --cleardb",
    "nodemon": "nodemon --config nodemon.json",
    "watch": "webpack --watch --mode development",
    "lint": "tslint --project tsconfig.json \"src/**/*.ts\"",
    "lint:fix": "tslint --fix --project tsconfig.json \"src/**/*.ts\"",
    "update:schema:jam": "cd dev/bin && ts-node update-jam-schema.ts",
    "update:schema:subsonic": "cd dev/bin && ts-node update-subsonic-schema.ts",
    "update:elasticmapping": "cd dev/bin && ts-node update-elastic-mapping.ts",
    "update:openapi:jam": "cd dev/bin && ts-node update-jam-openapi.ts",
    "update:openapi:subsonic": "cd dev/bin && ts-node update-subsonic-openapi.ts",
    "update:client:jam": "cd dev/bin && ts-node update-jam-client.ts",
    "update:router:jam": "cd dev/bin && ts-node update-jam-router.ts",
    "update:router:subsonic": "cd dev/bin && ts-node update-subsonic-router.ts",
    "update:jam": "npm run update:schema:jam && npm run update:openapi:jam && npm run update:client:jam && npm run update:router:jam",
    "update:subsonic": "npm run update:schema:subsonic && npm run update:openapi:subsonic && npm run update:router:subsonic",
    "update": "npm run update:jam && npm run update:subsonic && npm run update:elasticmapping",
    "docs:api:subsonic": "redoc-cli bundle src/model/subsonic-openapi.json --output ./dist/docs/api/subsonic/index.html",
    "docs:api:jam": "redoc-cli bundle src/model/jam-openapi.json --output ./dist/docs/api/jam/index.html",
    "clean": "rimraf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "release": "deplokay -c ./deplokay.json"
  },
  "dependencies": {
    "ajv": "6.10.2",
    "archiver": "3.1.1",
    "body-parser": "1.19.0",
    "colors": "1.3.3",
    "commander": "3.0.1",
    "common-password-checker": "0.1.0",
    "cookie-parser": "1.4.4",
    "cors": "2.8.5",
    "d3": "5.11.0",
    "d3-array": "2.3.1",
    "d3-node": "2.2.0",
    "d3-scale": "3.1.0",
    "d3-shape": "1.3.5",
    "elasticsearch": "16.3.0",
    "express": "4.17.1",
    "express-rate-limit": "5.0.0",
    "express-session": "1.16.2",
    "feedparser": "2.2.9",
    "fluent-ffmpeg": "2.1.2",
    "fs-extra": "8.1.0",
    "helmet": "3.21.0",
    "iconv-lite": "0.5.0",
    "jamp3": "0.4.0",
    "jimp": "0.6.8",
    "jsonwebtoken": "8.5.1",
    "limiter": "1.1.4",
    "md5-typescript": "1.0.5",
    "mime-types": "2.1.24",
    "moment": "2.24.0",
    "multer": "1.4.2",
    "nedb": "1.8.0",
    "on-finished": "^2.3.0",
    "passport": "0.4.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "request": "2.88.0",
    "seedrandom": "3.0.3",
    "sharp": "0.23.0",
    "svgo": "1.3.0",
    "tmp": "0.1.0",
    "waveform-data": "3.0.0",
    "winston": "3.2.1",
    "xml2js": "0.4.22"
  },
  "devDependencies": {
    "@types/archiver": "3.0.0",
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.6",
    "@types/d3": "5.7.2",
    "@types/elasticsearch": "5.0.35",
    "@types/express": "4.17.1",
    "@types/express-session": "1.15.14",
    "@types/feedparser": "2.2.3",
    "@types/fluent-ffmpeg": "2.1.10",
    "@types/fs-extra": "8.0.0",
    "@types/helmet": "0.0.44",
    "@types/jest": "^24.0.18",
    "@types/mime-types": "2.1.0",
    "@types/multer": "1.3.9",
    "@types/nedb": "1.8.8",
    "@types/nock": "10.0.3",
    "@types/node": "12.7.4",
    "@types/on-finished": "^2.3.1",
    "@types/passport": "1.0.1",
    "@types/passport-jwt": "3.0.1",
    "@types/passport-local": "1.0.33",
    "@types/request": "2.48.2",
    "@types/rimraf": "2.0.2",
    "@types/seedrandom": "2.4.28",
    "@types/sharp": "0.22.2",
    "@types/svgo": "1.2.1",
    "@types/tmp": "0.1.0",
    "@types/xml2js": "0.4.4",
    "concurrently": "4.1.2",
    "copy-webpack-plugin": "5.0.4",
    "deplokay": "0.0.9",
    "jest": "^24.9.0",
    "nock": "11.3.2",
    "nodemon": "1.19.2",
    "redoc-cli": "0.8.6",
    "rimraf": "3.0.0",
    "source-map-support": "0.5.13",
    "ts-jest": "^24.0.2",
    "ts-loader": "6.0.4",
    "ts-node": "8.3.0",
    "tslint": "5.19.0",
    "typescript": "3.6.2",
    "typescript-json-schema": "0.40.0",
    "webpack": "4.39.3",
    "webpack-cli": "3.3.7",
    "webpack-filter-warnings-plugin": "1.2.1",
    "webpack-node-externals": "1.7.2"
  },
  "engines": {
    "node": ">= 0.8.10"
  }
}
